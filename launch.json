{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "Debug Parking System",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/parking_system",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": false
          }
        ],
        "miDebuggerPath": "/usr/bin/gdb",
        "preLaunchTask": "build parking_system",
        "miDebuggerArgs": "",
        "serverStarted": "",
        "filterStderr": false,
        "filterStdout": false,
        "serverLaunchTimeout": 10000,
        "sourceFileMap": {
          "/mnt/c": "c:/"
        }
      }
    ]
  }
  